/**index.wxss**/
.content button{
    border-radius: 50%;
    padding: 16rpx;
    margin-left: 30rpx;
    font-weight: bold;
    width: 100rpx;
}

.content button.correct {
    border: 1px solid #25dc38;
}
.content button.half_correct {
    border: 1px solid #ffa500;
}
.content button.wrong {
    border: 1px solid #ff0000;
}



<!--index.wxml-->
<view class="page">
    <view class="page__bd">
        <view class="weui-panel weui-panel_access">
            <view class="weui-panel__bd">
                <navigator url="/pages/exam-detail/index?id={{item.id}}" class="weui-media-box weui-media-box_appmsg" hover-class="weui-cell_active"
                           wx:for="{{examList}}" wx:key="id" >
                    <view class="weui-media-box__hd weui-media-box__hd_in-appmsg">
                        <image class="weui-media-box__thumb" src="{{item.coverUrl}}" mode="aspectFill"/>
                    </view>
                    <view class="weui-media-box__bd weui-media-box__bd_in-appmsg">
                        <view class="weui-media-box__title weui-media-box__title_in-text">{{item.title}}</view>
                        <view class="weui-media-box__info">
                            <view class="weui-media-box__info__meta">{{item.enrollDatetime}}</view>
                        </view>
                    </view>
                </navigator>
            </view>
        </view>
    </view>
    <view class="page__ft">
        <view class="weui-loadmore weui-loadmore_line">
            <view class="weui-loadmore__tips weui-loadmore__tips_in-line">{{noDataLabel}}</view>
        </view>
    </view>
</view>

<!--ranking.wxml-->
<view class="page">
  <view class="weui-cells weui-cells_after-title">
      <view class="weui-cell">
        <view class="weui-cell__bd">我的成绩</view>
        <view class="weui-cell__ft">{{myScore}}分</view>
      </view>
      <view class="weui-cell">
        <view class="weui-cell__bd">我的名次</view>
        <view class="weui-cell__ft">第{{myRanking}}名</view>
      </view>
  </view>

  <view class="weui-panel">
    <view class="weui-panel__hd exam_title">成绩排行榜</view>
  </view>
  <view class="weui-cells weui-cells_after-title">
      <view class="weui-cell" wx:for="{{list}}" wx:key="id">
        <view class="weui-cell__bd">{{item.user.username}}</view>
        <view class="weui-cell__ft">{{item.score}}</view>
      </view>
  </view>
</view>


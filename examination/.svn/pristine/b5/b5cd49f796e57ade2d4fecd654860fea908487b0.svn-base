<!--historylist.wxml-->
<view class="page">
  <view class="weui-panel">
    <view class="weui-panel__hd exam_title">历史记录</view>

    <view class="weui-panel__bd">
      <view class="weui-media-box weui-media-box_text" wx:for="{{list}}" wx:key="id" bindtap="getDetail" id="{{item.id}}">
        <view class="weui-media-box__title weui-media-box__title_in-text">{{item.score}}分</view>
        <view class="weui-media-box__desc">考试时间：{{item.createdAt}}</view>
      </view>
    </view>
  </view>
</view>
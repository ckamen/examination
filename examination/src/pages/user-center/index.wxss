/**index.wxss**/
.page .header {
    background: #47bc62;
    width: 100%;
    height: 400rpx;
}

.page .header image {
    width: 150rpx;
    height: 150rpx;
    border-radius: 50%;
}

.header .upper {
    text-align: center;
    padding: 30rpx 0;
}

.img-wrapper {
    text-align:center;
    border-radius:50%;
    display:inline-block;
    width:150rpx;
    height:150rpx;
    border:10rpx solid #a3deb1;

}
.header .lower {
    color: #FFFFFF;
    text-align: center;
}

.body {
    padding: 200rpx 20rpx;
}
